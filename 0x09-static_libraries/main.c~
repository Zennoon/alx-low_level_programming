#include "main.h"

/**
 * main - The entry point of the program
 *
 * Description: Prints a string to the screen with the _puts function
 * which is defined inside the libmy static library
 * Return: Always 0 (Success)
 */
int main(void)
{
	_puts("\"At the end of the day, my goal was to be the best hacker\"\n\t- Kevin Mitnick");
	return (0);
}
